<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <import resource="/parent-context-one.xml" />
    <import resource="/parent-context-two.xml" />

    <bean id="parentPrinterTwo" class="com.jigsaw.spring.ParentPrinterTwoExtended" init-method="init" destroy-method="destroy"/>

    <bean id="mainPrinter" class="com.jigsaw.spring.MainPrinter" init-method="init" destroy-method="destroy">
        <property name="parentPrinterOne" ref="parentPrinterOne"/>
        <property name="parentPrinterTwo" ref="parentPrinterTwo"/>
    </bean>

</beans>