<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd">

    <import resource="jigsaw-context.xml"/>

    <bean id="jigsawPieceManager" class="com.core.manager.JigsawPieceManager">
        <property name="localRepository" value="target/test-classes"/>
        <property name="remoteRepositories" value="http://repo1.maven.org/maven2"/>
    </bean>

</beans>